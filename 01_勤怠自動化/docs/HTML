<script type="text/javascript" src="https://www.datadoghq-browser-agent.com/datadog-logs.js"></script>

	<script type="text/javascript">

    window.DD_LOGS && DD_LOGS.init({
        clientToken         : 'pub97e73deac342427094a658fc29b05877',
        site                : 'datadoghq.com',
        forwardErrorsToLogs : true,
        sampleRate          : 100
    });

    window.onerror = function(error_msg, url, linenumber, colnumber, error) {
        datadogLog(error_msg, error.stack, 'AC-9000', 'error');
    }

    function datadogLog(error_msg, stack_msg, code, status){
        var company_no      = '3993' > 0 ? '3993' : 'GUEST';
        var employee_no     = '65179' > 0 ? '65179' : 'GUEST';
        var company_plan    = 'BSC' ? 'BSC' : 'none';

        DD_LOGS.logger.log(
            error_msg,
            {
                stack_msg   : stack_msg,
                c_no        : company_no,
                e_no        : employee_no,
                plan        : company_plan,
                code        : code,
                request_id  : 'c22b3ee0b06acbfc631099586d3057b2',
                host        : 'vega-569ddd7b54-gztcc',
                service     : 'vega-front',
            },
            status
        );
    }

</script>


