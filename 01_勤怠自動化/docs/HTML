      <div class="cl-navigation-side"></div>
      <div class="cl-navigation-content">
        <div class="container" id="main-container">

        <script>
          // modify the layout as soon as dom is available
          // so that there won't be flicking of toggling navigation
          var expanded = window.CrowdLog.partial.getNavigationToggleValue();
          toggleNavigationExpanded(expanded);
        </script>


<script type="text/javascript">
$(document).ready(function() {
	$('.input-daterange').datepicker({
		format: "yyyy-mm-dd",
		weekStart: 0,
		maxViewMode: 2,
		language: "ja",
		autoclose: true,
		todayHighlight: true
	});

	$('#idDownloadForm button').click(function() {
		// export, char_code
		var v = $('select[name=export_type]').val().split('-');
		$('input[name=export]').val(v[0]);
		$('input[name=char_code]').val(v[1]);

		$('#idDownloadForm').submit();
	});
});
</script>
