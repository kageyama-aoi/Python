<script>
  var shouldPageFullWidth = "false" === "true";

  function showSideNavigation () {
    $('.cl-navigation-side').css('display', 'block');
    $('.cl-navigation-container').removeClass('fullscreen');
  }
  function hideSideNavigation () {
    $('.cl-navigation-side').css('display', 'none');
    $('.cl-navigation-container').addClass('fullscreen');
  }

  function toggleNavigationExpanded (expanded) {
    $('.cl-navigation-container').toggleClass('expanded', expanded);
    $('.cl-navigation-side').toggleClass('expanded', expanded);

    // dispatch resize event so that other part of UI
    // can react to layout change
    window.dispatchEvent(new Event('resize'));
  }

  function onTopOffsetChange (topOffset) {
    var availableHeight = 'calc(100vh - ' + topOffset + 'px)';
    // Need to supplement with dvh as ios safari needs it ...
    var availableHeightDvh = 'calc(100dvh - ' + topOffset + 'px)';
    var topPx = topOffset + 'px';
    $('.cl-navigation-container').css('min-height', availableHeight);
    $('.cl-navigation-side').css('top', topPx);
    $('.cl-navigation-side').css('height', availableHeight);

    // For multigantt and work dashboard
    $('#id_div_main').attr('style', 'height: ' + availableHeight + '; height: ' + availableHeightDvh + ';');
    if (window.inpm && window.inpm.mg) {
      inpm.mg.generatePageLayout();
    }
  }

  $(document).ready(function() {
    var navigationMountNode = document.querySelector('.cl-navigation-side');
    window.CrowdLog.partial.renderNavigation(navigationMountNode, {
      helpUrl: "https://help.crowdlog.jp/hc/ja/articles/4732624403870",
      onToggleExpanded: toggleNavigationExpanded,
      onTopOffsetChange: onTopOffsetChange
    });
  });
</script>

<div class="cl-notice-wrapper-container">
