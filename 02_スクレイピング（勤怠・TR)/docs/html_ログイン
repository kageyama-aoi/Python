    window.DD_LOGS && DD_LOGS.init({
        clientToken         : 'pub97e73deac342427094a658fc29b05877',
        site                : 'datadoghq.com',
        forwardErrorsToLogs : true,
        sampleRate          : 100
    });

    window.onerror = function(error_msg, url, linenumber, colnumber, error) {
        datadogLog(error_msg, error.stack, 'AC-9000', 'error');
    }

    function datadogLog(error_msg, stack_msg, code, status){
        var company_no      = '' > 0 ? '' : 'GUEST';
        var employee_no     = '0' > 0 ? '0' : 'GUEST';
        var company_plan    = '' ? '' : 'none';

        DD_LOGS.logger.log(
            error_msg,
            {
                stack_msg   : stack_msg,
                c_no        : company_no,
                e_no        : employee_no,
                plan        : company_plan,
                code        : code,
                request_id  : '62cf206535bb9be8971a2418a598340e',
                host        : 'vega-569ddd7b54-5722l',
                service     : 'vega-front',
            },
            status
        );
    }

</script>


