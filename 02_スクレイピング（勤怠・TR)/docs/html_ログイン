$(document).ready(function() {
	$('input[name="email"]').focus();

	$('#idLoginForm button').submit(function() {
		if (!$('#idLoginForm').validationEngine('validate')) {
			return false;
		}
	});

	$('#idLoginForm').validationEngine({
	});
});

// create crypt object for OAuth state
var cryptObj = window.crypto || window.msCrypto;
function guid() {
	var buf = new Uint16Array(8);
	cryptObj.getRandomValues(buf);
	function s4(num) {
		var ret = num.toString(16);
		while (ret.length < 4){
			ret = '0' + ret;
		}
		return ret;
	}
	return s4(buf[0]) + s4(buf[1]) + '-' + s4(buf[2]) + '-' + s4(buf[3]) + '-' + s4(buf[4]) + '-' + s4(buf[5]) + s4(buf[6]) + s4(buf[7]);
}

function login_microsoft() {
	oauth_state = guid();
	$.blockUI();
	$.ajax({
		type: 'POST',
		url: '/microsoft.cgi',
		data: {json: true, oauth_state: oauth_state},
		dataType: 'json',
		cache: false
	}).done(function(response){
		window.location.href = response.redirectURL;
	}).fail(function(){
		alert('Error');
	}).always(function(){
		$.unblockUI();
	});
}

</script>

